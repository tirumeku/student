datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Student {
  id        String   @id @default(cuid())
  name      String
  studentId String   @unique
  course    String
  interests String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
